{
  "printWidth": 80,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "^react-router-dom$",
    "<THIRD_PARTY_MODULES>",
    "^public/(.*)$",
    "^app/(.*)$",
    "^pages$",
    "^widgets/(.*)$",
    "^features/(.*)$",
    "^shared/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true
}
